obj-$(CONFIG_LGE_TOUCH_LGSIC_LG4946)		+= touch_lg4946_asc.o
obj-$(CONFIG_LGE_TOUCH_LGSIC_LG4946)		+= touch_filter.o

obj-$(CONFIG_LGE_TOUCH_LGSIC_SW49407)		+= touch_sw49407.o
