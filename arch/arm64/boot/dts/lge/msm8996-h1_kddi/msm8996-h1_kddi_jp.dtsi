/ {
	reserved-memory {
		ramoops_mem: ramoops_region@c0000000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0xc0000000 0 0x100000>;
		};

		crash_fb_mem: crash_fb_mem@c0100000 {
			linux,contiguous-region;
			reg = <0 0xc0100000 0 0xc00000>;
			label = "crash_fb_mem";
		};

		cont_splash_mem: cont_splash_mem@83401000 {
			status = "disabled";
		};

		/delete-node/ dfps_data_mem@83400000;
	};
};

&soc {
	wdog: qcom,wdt@9830000 {
		qcom,bark-time = <16000>;
	};

	ramoops {
		compatible = "ramoops";
		mem-size = <0x80000>;
		mem-address = <0xc0000000>;
		record-size = <4096>;
		console-size = <0x40000>;
		ftrace-size = <0x40000>;
		dump-oops = <1>;
	};

	crash_fb {
		compatible = "crash_fb";
		mem-addr = <0xc0100000>;
		mem-size = <0xc00000>;
	};

	bootlog {
		compatible = "bootlog";
	};

	earjack-debugger {
		compatible = "serial,earjack-debugger";
		status = "ok";
		interrupt-parent = <&tlmm>;
		serial,irq-gpio = <&tlmm 126 0>;
	};

	lge,monitor-thermal {
		compatible = "lge,monitor-thermal";
		status = "ok";
		lge,hot-poll-time = <10000>;
		lge,hot-crit-temp = <48>;
		lge,poll-time = <60000>;
		qcom,monitor-thermal-vadc = <&pm8994_vadc>;
	};

	/* I2C configuration */
/*	i2c_12: i2c@75ba000 {
		pinctrl-names = "default", "i2c_sleep";
		pinctrl-0 = <&i2c_12_default>;
		pinctrl-1 = <&i2c_12_sleep>;
	};
*/
	i2c_7: i2c@75b5000 {
		pinctrl-names = "default", "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_7_default>;
		pinctrl-1 = <&i2c_7_active>;
		pinctrl-2 = <&i2c_7_sleep>;
	};

	i2c_6: i2c@757a000 {
		pinctrl-names = "default", "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_6_default>;
		pinctrl-1 = <&i2c_6_active>;
		pinctrl-2 = <&i2c_6_sleep>;
	};

	apcc_cpr: cpr3-ctrl@99e8000 {
		thread@1 {
			apc1_vreg: regulator {
				qcom,cpr-closed-loop-voltage-fuse-adjustment =
					/* Speed bin 0 */
					<35000 35000 40000 (-30000) 70000>,
					<    0     0     0 (-70000) 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0    10000 50000>,
					<    0     0     0    10000 40000>,
					<    0     0     0    10000 40000>,
					<    0     0     0        0 30000>,

					/* Speed bin 1 */
					<35000 35000 40000 (-30000) 70000>,
					<    0     0     0 (-70000) 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,

					/* Speed bin 2 */
					<35000 35000 40000 (-30000) 70000>,
					<    0     0     0 (-70000) 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,

					/* Speed bin 3 */
					<35000 35000 40000 (-30000) 70000>,
					<    0     0     0 (-70000) 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>,
					<    0     0     0        0 30000>;
			};
		};

		thread@0 {
			apc0_pwrcl_vreg: regulator-pwrcl {
				qcom,cpr-closed-loop-voltage-fuse-adjustment =
					/* Speed bin 0 */
					<35000 35000 40000  40000   70000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 5000 35000>,
					<20000 10000  5000 5000 35000>,
					<20000 10000  5000 5000 35000>,
					<20000 10000  5000 (-5000) 25000>,

					/* Speed bin 1 */
					<35000 35000 40000  40000   70000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,

					/* Speed bin 2 */
					<35000 35000 40000  40000   70000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,

					/* Speed bin 3 */
					<35000 35000 40000  40000   70000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>,
					<20000 10000  5000 (-5000) 25000>;
			};
		};
	};

	/*delete QCT finger */
	/delete-node/ qcom,qbt1000;

	/delete-node/ qcom,cnss;
	/delete-node/ qcom,wil6210;

};

