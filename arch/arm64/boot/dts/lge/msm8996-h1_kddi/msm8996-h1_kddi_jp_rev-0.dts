/dts-v1/;

#include "../../qcom/msm8996-v2.dtsi"
#include "msm8996-h1_kddi_jp.dtsi"
#include "../msm8996-touch-synaptics-s3320.dtsi"
#include "msm8996-h1_kddi_jp-camera-rev-0.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM 8996 v2 + PMI8994 MTP";
	compatible = "qcom,msm8996-mtp", "qcom,msm8996", "qcom,mtp";
	qcom,board-id = <0x308 0>;
};

/{
	aliases {
		spi12 = &spi_12;
	};
	soc {
		spi_12: spi@75ba000{    /*BLSP-2 QUP5 */
			compatible = "qcom,spi-qup-v2";
			status = "disabled";
		};
	};
};

&i2c_6 {
	analogix_anx7816@72 {
		status = "disabled";
	};
};

&spi_12 {
	lg4945@1 {
		status = "disabled";
	};
};

&soc {
	gpio_keys {
		vol_up {
			gpios = <&pm8994_gpios 3 0x1>;
		};
	};
};

&pm8994_gpios {
	gpio@c200 { /* GPIO 3 */
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;           /* Enable GPIO */
		status = "okay";
	};
};

/* Display Function */
&soc {
	dsi_panel_pwr_supply: dsi_panel_pwr_supply {
		#address-cells = <1>;
		#size-cells = <0>;

		qcom,panel-supply-entry@0 {
			reg = <0>;
			qcom,supply-name = "lab";
			qcom,supply-min-voltage = <4600000>;
			qcom,supply-max-voltage = <6000000>;
			qcom,supply-enable-load = <100000>;
			qcom,supply-disable-load = <100>;
		};

		qcom,panel-supply-entry@1 {
			reg = <1>;
			qcom,supply-name = "ibb";
			qcom,supply-min-voltage = <4600000>;
			qcom,supply-max-voltage = <6000000>;
			qcom,supply-enable-load = <100000>;
			qcom,supply-disable-load = <100>;
			qcom,supply-post-on-sleep = <20>;
		};

		qcom,panel-supply-entry@2 {
			reg = <2>;
			qcom,supply-name = "tpvci";
			qcom,supply-min-voltage = <3000000>;
			qcom,supply-max-voltage = <3000000>;
			qcom,supply-enable-load = <100000>;
			qcom,supply-disable-load = <100>;
			qcom,supply-post-on-sleep = <1>;
		};

		qcom,panel-supply-entry@3 {
			reg = <3>;
			qcom,supply-name = "tpio";
			qcom,supply-min-voltage = <1800000>;
			qcom,supply-max-voltage = <1800000>;
			qcom,supply-enable-load = <100000>;
			qcom,supply-disable-load = <100>;
			qcom,supply-post-on-sleep = <1>;
		};
	};
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&dsi_dual_lgd_cmd>;
	qcom,platform-dsv-ena-gpio = <&tlmm 108 0>;

	lge,extra-gpio-names = "vddio";

	lge,gpio-vddio = <&tlmm 122 0>;

	tpio-supply = <&pm8994_l14>;
	tpvci-supply =  <&pm8994_l22>;
};

&mdss_dsi1 {
	qcom,dsi-pref-prim-pan = <&dsi_dual_lgd_cmd>;
	qcom,platform-dsv-ena-gpio = <&tlmm 108 0>;

	lge,extra-gpio-names = "vddio";

	lge,gpio-vddio = <&tlmm 122 0>;

	tpio-supply = <&pm8994_l14>;
	tpvci-supply =  <&pm8994_l22>;
};

/* RPM controlled regulators: */
&rpm_bus {
	rpm-regulator-ldoa14 {
		status = "okay";
		pm8994_l14: regulator-l14 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			qcom,init-voltage = <1800000>;
			status = "okay";
		};
	};

	rpm-regulator-ldoa22 {
		status = "okay";
		pm8994_l22: regulator-l22 {
			regulator-min-microvolt = <3000000>;
			regulator-max-microvolt = <3000000>;
			qcom,init-voltage = <3000000>;
			status = "okay";
		};
	};
};

&i2c_6 {
    p1_dsv@3e {
        status = "ok";
        compatible = "p1_dsv";
        reg = <0x3e>;
    };
};

/* pinctrl for Display function
 * GPIO108 : DSV_ENA
 * GPIO122 : LCD_VDD_IO_EN
 */
&soc {
	tlmm: pinctrl@01010000 {
		compatible = "qcom,msm8996-pinctrl";
		reg = <0x01010000 0x300000>;
		interrupts = <0 208 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;

		pmx_mdss: pmx_mdss {
			mdss_dsi_active1: mdss_dsi_active1 {
				mux {
					pins = "gpio108";
					function = "gpio";
				};

				pmx_mdss {
					pins = "gpio108";
					drive-strength = <8>; /* 8 mA */
					bias-disable = <0>; /* no pull */
					output-high;
				};
			};
			mdss_dsi_active2: mdss_dsi_active2 {
				mux {
					pins = "gpio122";
					function = "gpio";
				};

				pmx_mdss {
					pins = "gpio122";
					drive-strength = <8>; /* 8 mA */
					bias-disable = <0>; /* no pull */
					output-high;
				};
			};
			mdss_dsi_suspend1: mdss_dsi_suspend1 {
				mux {
					pins = "gpio108";
					function = "gpio";
				};

				config {
					pins = "gpio108";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* pull down */
				};
			};
			mdss_dsi_suspend2: mdss_dsi_suspend2 {
				mux {
					pins = "gpio122";
					function = "gpio";
				};

				config {
					pins = "gpio122";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* pull down */
				};
			};
		};
	};
};

&pmi8994_charger {
    lge,cable-adc-useset  = <3>;
    lge,cable-adc-muxset  = <0x79>;
};

&sdhc_2 {
    cd-gpios = <&tlmm 60 0x1>; /* 1: Active-Low, 0: Active-High */
};

