//[H1_BT_BRINGUP_S]
                /* gpio pin mux for bluetooth enable pin */
		i2c_3 {
			i2c_3_active: i2c_3_active {
				mux {
					pins = "gpio47", "gpio48";
					function = "blsp_i2c3";
				};

				config {
					pins = "gpio47", "gpio48";
					drive-strength = <2>;
					bias-disable;
				};
			};

			i2c_3_sleep: i2c_3_sleep {
				mux {
					pins = "gpio47", "gpio48";
					function = "blsp_i2c3";
				};

				config {
					pins = "gpio47", "gpio48";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};
		/* gpio pin mux for bluetooth host wake pin */
                pmx_bt_default {
			bt_default: bt_default {
				mux {
					pins = "gpio24", "gpio92";
					function = "gpio";
				};

	    			config {
	    				pins = "gpio24", "gpio92";
	    				drive-strength = <2>;
	    				bias-disable = <0>;
	    			};
	                };
		};
//[H1_BT_BRINGUP_E]

		pmx_mdss: pmx_mdss {
			mdss_dsi_active: mdss_dsi_active {
				mux {
					pins = "gpio8";
					function = "gpio";
				};

				pmx_mdss {
					pins = "gpio8";
					drive-strength = <8>; /* 8 mA */
					bias-disable = <0>; /* no pull */
					output-high;
				};
			};
			mdss_dsi_suspend: mdss_dsi_suspend {
				mux {
					pins = "gpio8";
					function = "gpio";
				};

				config {
					pins = "gpio8";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* pull down */
				};
			};

			mdss_dsi_active3: mdss_dsi_active3 {
				mux {
					pins = "gpio121";
					function = "gpio";
				};

				pmx_mdss {
					pins = "gpio121";
					drive-strength = <2>; /* 2 mA */
					bias-pull-down;  /* pull down */
				};
			};
			mdss_dsi_suspend3: mdss_dsi_suspend3 {
				mux {
					pins = "gpio121";
					function = "gpio";
				};

				config {
					pins = "gpio121";
					drive-strength = <2>; /* 2 mA */
					bias-bus-hold;
				};
			};
		};

		mdss_te_active: mdss_te_active {
			mux {
				pins = "gpio10";
				function = "mdp_vsync";
			};
			config {
				pins = "gpio10";
				drive-strength = <2>; /* 8 mA */
				bias-pull-down; /* pull down*/
			};
		};


		mdss_te_suspend: mdss_te_suspend {
			mux {
				pins = "gpio10";
				function = "mdp_vsync";
			};
			config {
				pins = "gpio10";
				drive-strength = <2>; /* 2 mA */
				bias-pull-down; /* pull down */
			};
		};

		/* gpio pin mux for back-up battery charging status pin */
		pmx_chg_stat {
			chg_stat_active: chg_stat_active {
				mux {
					pins = "gpio45";
					function = "gpio";
				};
				config {
