			"battery", "charge_full", "/sys/class/power_supply/bms/charge_full",
			"battery", "charge_now_raw", "/sys/class/power_supply/bms/charge_now_raw",
			"battery", "cycle", "NULL",
			"battery", "battery_type", "/sys/class/power_supply/bms/battery_type",
